<knimeNode icon="./moz.png" type="Manipulator" xmlns="http://knime.org/node/v2.8" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://knime.org/node/v2.10 http://knime.org/node/v2.10.xsd">
    <name>Moz Link Metrics</name>
    
    <shortDescription>
        Returns Moz Link Metrics data via Moz API
    </shortDescription>
    
    <fullDescription>
        <intro>This node uses Moz API Connector to connect to Moz API to pull Link Metrics data</intro>
        <option name="Link">The Link to analyze.</option>
    </fullDescription>
    
    <ports>
		<inPort index="0" name="Links to Analyze">
			Table with URL's to Analyze
		</inPort>
        <inPort index="1" name="Moz API Connection" optional="true">
          An optional Moz Connection that should be used instead of providing the connection information in the dialog.
        </inPort>
        <outPort index="0" name="Moz Link Metrics Data">Moz Link Metrics data in table form</outPort>
    </ports>    
</knimeNode>
